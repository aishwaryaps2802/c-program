#include <stdio.h>


int main()
{
    int marks[6],pass=0,fail=0;
    for(int i=0;i<6;i++){
         printf("enter marks of a student %d:",i+1);
         scanf("%d",&marks[i]);

    if(marks[i] >= 35)
        pass++;

    else
        fail++;
    }


    printf("\nPASSESD:%d\nFAILED:%d\n",pass,fail);
    return 0;

}
  
